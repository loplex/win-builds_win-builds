export BUILD_TRIPLET="x86_64-slackware-linux"
export HOST_TRIPLET="${BUILD_TRIPLET}"
export TARGET_TRIPLET="i686-w64-mingw32"

export HOST_STRIP="strip"
export TARGET_STRIP="${TARGET_TRIPLET}-strip"

export HOST_EXE_FORMAT="ELF"
export TARGET_EXE_FORMAT="PE"

export YYPREFIX="/opt/mgw32_toolchain"
export YYPREFIX_libs="/opt/mgw32_libs"
export PATH=${YYPREFIX}/bin:${YYPREFIX}/sbin:$PATH
export LD_LIBRARY_PATH=${YYPREFIX}/lib64
export YYOUTPUT="${CWD}/output/toolchain"

